  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "./build",
    "types": ["jasmine", "node", "protractor"]
  },
  "exclude": [
    "../node_modules"
  ]
}